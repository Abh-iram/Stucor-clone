          <input class="form-control" type="file" name="Notes" />   <!--id="formFileMultiple" -->
        </div>
        <div class="form-group">
          <label for="formGroupExampleInput">QUESTIONS</label>
          <input type="file" class="form-control" name="Questions" />
        </div>
        <div>
          <button type="submit" class="btn btn-primary">Add</button>
        </div>

      </form>
    </div>
  </div>
</section>