<section class="btm-footer">
    <div class="container">
        <div class="pt-4 text-center">
            <h4 class="text-primary">Choose Your Regulation</h4>
        </div>
        <div class="pt-3">
            <marquee behavior="scroll" direction="left">Welcome to the cloned version of STUCOR Digital Platform for Anna University students</marquee>
        </div>
        <div class="row pt-5 d-flex justify-content-center">
            <div class="col-md-12">
                <table class="table table-bordered text-center">
                    <tbody>
                        {{#each regulation}}
                         <tr>
                            <td>{{this.regulation}}</td>
                            <td><a href="{{this.page}}">CLICK HERE</a></td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>